package programizeexamples;
import java.util.Scanner;
public class ReverseNumber {
	public static void main(String[] args) {
		System.out.println("enter a no");
		Scanner sc =new Scanner(System.in);
		int b =sc.nextInt();
		int a=0;
		for (int i=0;i<=6;i++) {
			 a= a*10+(b%10);
			 b=b/10;
		}
		System.out.println(a);
	}
}
