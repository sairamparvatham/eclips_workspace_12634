import java.util.Scanner;

public class comparigenOf3Num {

	public static void main(String[] args) {
		int m;
		int l;
		int z;
		Scanner sc = new Scanner(System.in);
		m=sc.nextInt();
		l=sc.nextInt();
		l=sc.nextInt();
		int max=(m>l) ? m : l;
		int r= max
		int fmax=(r > z) ? max : z;
		System.out.println(fmax);

	}

}
